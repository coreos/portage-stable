ARCH="m68k"
ACCEPT_KEYWORDS="m68k"

CHOST="m68k-unknown-linux-gnu"
CFLAGS="-O2 -pipe"
CXXFLAGS="${CFLAGS}"
FFLAGS="${CFLAGS}"
FCFLAGS="${CFLAGS}"

# Disable sandbox on this architecture
FEATURES="-sandbox"
