ARCH="x86-macos"
CHOST="i686-apple-darwin8"
ACCEPT_KEYWORDS="~x86-macos"

USE="mmx mmxext sse sse2"
# set arch to prescott for all MacTel users, as this enables SSE and MMX
# support in GCC
CFLAGS="-march=prescott"
