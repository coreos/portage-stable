ARCH="x64-macos"
CHOST="x86_64-apple-darwin9"
ACCEPT_KEYWORDS="~x64-macos"

USE="mmx mmxext sse sse2"
# set arch to nocona for all MacTel users, as this enables SSE and MMX
# support in GCC, nocona = Core2Duo = 64-bits
CFLAGS="-march=nocona"
